[global]
ioengine=libaio
direct=1
time_based
norandommap
group_reporting
disk_util=0
continue_on_error=all
rate_process=poisson
bs=8k
rw=randread
iodepth=128

[db-disk1]
size=64G
filename=/dev/sdb

[db-disk2]
size=64G
filename=/dev/sdc

[db-disk3]
size=64G
filename=/dev/sdd

[db-disk4]
size=64G
filename=/dev/sde
